  <SQLQuery>
    <![CDATA[SELECT WPSTYLESETUPEO.STYLE_SETUP_ID, 
       WPSTYLESETUPEO.SYSTEM_ID,
       POC.LC_UNIT,  
         APPS.ORG_ID_TO_ORG_NAME(LC_UNIT) LC_UNIT_NAME,       
        POC.BUYER_ID, 
        APPS.XX_OM_BUYER_NAME_FOR_ID(POC.BUYER_ID) BUYER_NAME ,
        POC.SEASON, 
        POC.STYLE ,   
        POC.COLOR, 
        POC.WASH_NAME , 
       POC.ORDER_QTY ,       
       WPSTYLESETUPEO.PRODUCTION_UNIT, 
       APPS.ORG_ID_TO_ORG_NAME(LC_UNIT) PRODUCTION_UNIT_NAME,   
       WPSTYLESETUPEO.PB_FLAG, 
       WPSTYLESETUPEO.CREATED_BY, 
       WPSTYLESETUPEO.CREATION_DATE, 
       WPSTYLESETUPEO.LAST_UPDATED_BY, 
       WPSTYLESETUPEO.LAST_UPDATED_DATE, 
       WPSTYLESETUPEO.SAM_VERSION, 
       WPSTYLESETUPEO.ORG_ID,
WPSTYLESETUPEO.PROJECTION_STYLE,
     (
        SELECT  nvl(  MAX(SV.SAM_VERSION),0) FROM  WP_STYLE_WISE_PROCESS_SAM_V SV  WHERE  SV.SYSTEM_ID = WPSTYLESETUPEO.SYSTEM_ID
        AND  SV.ORG_ID =  WPSTYLESETUPEO.ORG_ID
               
      )  AVAILABLE_SAM_VERSION            
       
    FROM WP_STYLE_SETUP   WPSTYLESETUPEO,
    (  SELECT PV.SYSTEM_ID, 
        PV.LC_UNIT, 
        PV.BUYER_ID, 
        PV.SEASON, 
        PV.STYLE ,   
        PV.COLOR, 
        PV.WASH_NAME , 
        PV.ORDER_QTY 
        FROM POC_SYSTEM_ID_VIEW PV
      ) POC  
     WHERE WPSTYLESETUPEO.SYSTEM_ID = POC.SYSTEM_ID]]>
  </SQLQuery>
