<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="WashTypeLOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.lov.WashTypeLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select 'Normal Rinse'   WASH_TYPE   , 1 seq  from dual
union
select 'Enz/Bleach' WASH_TYPE , 2 seq  from dual
union 
select 'Acid/Towel' WASH_TYPE,  3 seq from dual
union
select 'Overdye' WASH_TYPE ,  4 seq from dual
union
select 'Gmts Wash' WASH_TYPE ,  5 seq from dual

order by seq]]>
  </SQLQuery>
  <ViewAttribute
    Name="WashType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
