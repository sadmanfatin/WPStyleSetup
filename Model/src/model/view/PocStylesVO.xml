  <SQLQuery>
    <![CDATA[SELECT DISTINCT
POCL.SYSTEM_ID, 
POCH.LC_UNIT, 
  APPS.ORG_ID_TO_ORG_NAME(POCH.LC_UNIT) LC_UNIT_NAME, 
POCH.LC_UNIT PRODUCTION_UNIT, 
  APPS.ORG_ID_TO_ORG_NAME(POCH.LC_UNIT)   PRODUCTION_UNIT_NAME,
POCH.BUYER_ID, 
 APPS.XX_OM_BUYER_NAME_FOR_ID(POCH.BUYER_ID) BUYER_NAME ,
POCH.SEASON, 
POCH.STYLE ,   
POCL.COLOR, 
POCL.WASH_NAME , 
POCH.ORDER_QTY ,
(
SELECT  MAX(SV.SAM_VERSION) FROM WP_STYLE_WISE_PROCESS_SAM_V SV  WHERE  SV.SYSTEM_ID = POCL.SYSTEM_ID
AND  SV.ORG_ID = :p_org_id
)   SAM_VERSION

FROM XX_OM_POC_H_T POCH,
XX_OM_POC_L_T POCL 
WHERE POCH.POC_ID = POCL.POC_ID
AND POCH.GM_APPROVAL = 'Y'
AND POCH.CREATION_DATE > SYSDATE-360
AND POCL.SYSTEM_ID IS NOT NULL
AND NOT EXISTS (SELECT  STS.SYSTEM_ID FROM WP_STYLE_SETUP STS WHERE STS.SYSTEM_ID =  POCL.SYSTEM_ID  
AND STS.ORG_ID = :p_org_id
)
ORDER BY POCL.SYSTEM_ID DESC]]>
