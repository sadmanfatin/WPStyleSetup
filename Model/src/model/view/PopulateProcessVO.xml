  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.view.PopulateProcessVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_system_id"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT SI.ORG_ID,
       SI.WP_SECTION_ID,
       SI.UNIT_SECTION_ID,
       SI.SECTION_NAME,
       SI.SERIAL,
       PI.WP_PROCESS_ID,
       PI.UNIT_PROCESS_ID,
       PI.PROCESS_NAME,
       SI.SECTION_TYPE
  FROM WP_SECTION_INFO SI, WP_PROCESS_INFO PI
 WHERE SI.WP_SECTION_ID = PI.WP_SECTION_ID
 AND  NOT EXISTS (SELECT 1 
                    FROM WP_STYLE_WISE_PROCESS_SAM  SPS
                        WHERE SPS.SYSTEM_ID  = :p_system_id  
                        and  SPS.WP_PROCESS_ID = PI.WP_PROCESS_ID 
                     )             
 ORDER BY    SI.UNIT_SECTION_ID ,    PI.UNIT_PROCESS_ID]]>
  </SQLQuery>
